datasource postgres {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  firstName String?
  lastName  String?
  username  String   @unique
  password  String
  roles     Json
}

model KarzaPan {
  id             String   @id @default(cuid())
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  panNumber      String
  reqPayload     Json
  resPayload     Json?
  errorMessage   String?
  httpStatusCode String?
  errorCode      String?
  customerId     Int      @unique
  panName        String?
}

model KarzaEmployment {
  resPayload      Json
  errorMessage    String?
  httpStatusCode  String
  reqPayload      Json
  karzaStatusCode String?
  id              String   @id @default(cuid())
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
  customerId      Int
  karzaStatus     String?
}

model HyperVergeAadhaar {
  id               String   @id @default(cuid())
  createdAt        DateTime @default(now())
  updatedAt        DateTime @updatedAt
  errorMessage     String?
  customerId       Int      @unique
  reqPayload       Json
  httpStatusCode   String
  resPayload       Json?
  adhaarNumber     String
  errorCode        String?
  reqType          String
  hyperVergeStatus String?
}

model KarzaAdhaarVerify {
  id             String   @id @default(cuid())
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  customerId     Int      @unique
  adhaarNumber   Int      @unique
  reqPayload     Json?
  resPayload     Json?
  httpStatusCode Int?
}

model KarzaVerifyAdhaar {
  errorMessage   String?
  id             String   @id @default(cuid())
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  customerId     Int      @unique
  adhaarNumber   Int      @unique
  resPayload     Json?
  httpStatusCode Int
  reqPayload     Json
  errorCode      String?
  statusCode     String
  reqType        String
}

model HyperVergeKycFailureHistory {
  customerId     Int?     @unique
  reqPayload     Json?    @unique
  id             String   @id @default(cuid())
  resPayload     Json?    @unique
  httpStatusCode Int      @unique
  errorCode      Int      @unique
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  errorMessage   String
}

model HyperVergeOtp {
  id             String   @id @default(cuid())
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  customerId     Int      @unique
  reqPayload     Json
  resPayload     Json?
  httpStatusCode String
  errorCode      String?
  errorMessage   String?
}

model HyperVergeKycSuccess {
  aadhaarNumber Int?      @unique
  aadhaarDob    DateTime?
  panNumber     String?   @unique
  panDob        DateTime?
  resPayload    Json?     @unique
  customerId    Int?      @unique
  id            String    @id @default(cuid())
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
  mobileNumber  Int?      @unique
  reqPayload    Json?     @unique
}

model KnowlarityMakeCall {
  agentId              Int      @unique
  agentMobileNumber    Int
  resPayload           Json
  customerId           Int      @unique
  errorCode            String
  customerMobileNumber Int      @unique
  reqPayload           Json?
  errorMessage         String
  id                   String   @id @default(cuid())
  createdAt            DateTime @default(now())
  updatedAt            DateTime @updatedAt
  httpStatusCode       String?
}

model ExperianCreditBureau {
  httpStatusCode     String?
  errorMessage       String?
  errorCode          String?
  customerId         Int
  reqPayload         Json
  resPayload         Json?
  id                 String   @id @default(cuid())
  createdAt          DateTime @default(now())
  updatedAt          DateTime @updatedAt
  reqType            String?
  experianStatusCode String?
}

model CashfreePaymentGateway {
  reqPayload     Json?
  resPayload     Json
  errorMessage   String
  errorCode      String
  customerId     Int?     @unique
  id             String   @id @default(cuid())
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  httpStatusCode String?
}
